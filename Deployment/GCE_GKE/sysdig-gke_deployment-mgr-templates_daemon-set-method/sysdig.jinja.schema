required:
- cluster
- key
- delegatedNode
- apiUri

properties:
  cluster:
    type: string
    description: Cluster name. Requires that the type "<cluster>-extensions-v1beta1" exists.
  key:
    type: string
    description: Sysdig key.
  delegatedNode:
    type: string
    description: IP address for delegated node.
  apiUri:
    type: string
    description: URI of the apiserver.

